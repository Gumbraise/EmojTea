{
    "name": "EmojTea",
    "version": "0.1",
    "description": "EmojTea Chrome plugin",
    "manifest_version": 2,
    "permissions": ["activeTab", "tabs"],
    "content_scripts": [{
      "js": ["content.js"],
      "matches": ["<all_urls>"]
    }],
    "browser_action": {
      "default_popup": "popup.html"
    }
}